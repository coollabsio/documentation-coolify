#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/vite/bin/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/vite/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/node_modules:/mnt/c/Users/justserdar/Documents/node_modules:/mnt/c/Users/justserdar/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/vite/bin/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/vite/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/node_modules:/mnt/c/Users/justserdar/Documents/node_modules:/mnt/c/Users/justserdar/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
