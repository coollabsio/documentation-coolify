  export NODE_PATH="/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/esbuild/bin/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/esbuild/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/node_modules:/mnt/c/Users/justserdar/Documents/node_modules:/mnt/c/Users/justserdar/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/esbuild/bin/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/esbuild/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/node_modules:/mnt/c/Users/justserdar/Documents/node_modules:/mnt/c/Users/justserdar/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/justserdar/Documents/GitHub/coolify-new-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
